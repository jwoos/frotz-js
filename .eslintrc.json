{
	"env": {
		"es6": true,
		"node": true,
		"jasmine": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": [
			"error",
			"tab"
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"always"
		],
		"no-console": [
			"error",
			{
				"allow": ["warn", "error"]
			}
		]
	}
}
